            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inplanosobra" value="#{Supervision$Externa.planosobra}"/>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titespecifictecnicas" value="#{var.especifictecnicas}"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inespecifictecnicas" value="#{Supervision$Externa.especifTecnica}"/>
            </rich:panel>
        </h:panelGrid>

    </rich:popupPanel>


</html>
