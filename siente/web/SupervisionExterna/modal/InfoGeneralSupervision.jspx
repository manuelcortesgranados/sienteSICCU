            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inAdiciones"  value="#{Supervision$Externa.objetoSeguimiento.numvlradicion}" >
                    <f:convertNumber pattern="$ ###,###.##" />
                </h:outputText>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titvalorTotal" value="#{var.valortotal}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="invalorTotal" value="#{Supervision$Externa.objetoSeguimiento.numvlrtotal}">
                    <f:convertNumber pattern="$ ###,###.##" />
                </h:outputText>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titrelacioncausalidad" value="#{var.relacioncausalidad}"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inrelacioncausalidad" value="#{Supervision$Externa.relacioncasualInforme}"/>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titobrarte" value="#{var.obrarte}"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inobrarte" value="#{Supervision$Externa.obradearte}"/>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titdisenios" value="#{var.disenios}"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="indisenios" value="#{Supervision$Externa.disenios}"/>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titpermisoslicencias" value="#{var.permisoslicencias}"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inpermisoslicencias" value="#{Supervision$Externa.permisosLicencias}"/>
