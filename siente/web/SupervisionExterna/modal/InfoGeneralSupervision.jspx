            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="invalorContrato" value="#{Supervision$Externa.objetoSeguimiento.numvlrcontrato}" >
                    <f:convertNumber pattern="$ ###,###.##" />
                </h:outputText>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titContratista" value="#{var.contratista}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inContratista" value="#{Supervision$Externa.objetoSeguimiento.strcontratista}" />
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titPlazoMeses" value="#{var.houtplazomeses}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inPlazoMeses"  value="#{Supervision$Externa.objetoSeguimiento.intplazocontrato}"/>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titFechaActaInicio" value="#{var.fechainicioacta}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inFechaActaInicio" value="#{Supervision$Externa.objetoSeguimiento.datefecactaini}"/>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titFechaFinalizacion" value="#{var.fechafinalizacion}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inFechaFinalizacion" value="#{Supervision$Externa.objetoSeguimiento.datefecactafin}"/>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titProrroga" value="#{var.prorroga}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inProrroga" value="#{Supervision$Externa.objetoSeguimiento.intmesesprorroga}" />
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titFechaFinalizacionProrroga" value="#{var.fechafinalizacionprorroga}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="inFechaFinalizacionProrroga" value="#{Supervision$Externa.objetoSeguimiento.datefecfinprorroga}" />
