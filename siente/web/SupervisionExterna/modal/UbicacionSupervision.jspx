            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="verLatitudNorte" value="#{Supervision$Externa.objetoSeguimiento.strlatitud}"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titLatitudEste" value="#{var.longitudeste}" styleClass="tooltip"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="verLongitudEste" value="#{Supervision$Externa.objetoSeguimiento.strlongitud}"/>
            </rich:panel>
        </h:panelGrid>
    </rich:popupPanel>

</html>

