<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:a4j="http://richfaces.org/a4j" xmlns:interkont="http://www.interkont.com/jsf"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:rich="http://richfaces.org/rich"
      xmlns:ui="http://java.sun.com/jsf/facelets">
    <rich:popupPanel id="modalimpactosocial"  headerClass="rich-panel-header" height="148" width="354" moveable="false">
        <f:facet name="header" >
            <h:panelGroup >
                <h:outputText value="Impacto Social" styleClass="txt_header"/>
            </h:panelGroup>
        </f:facet>
        <f:facet name="controls">
            <h:form>
                <h:panelGroup>
                    <a4j:commandButton id="esconder-modalimpactosocial" onclick="javascript:Richfaces.hideModalPanel('modalimpactosocial')" immediate="true"
                                       styleClass="btn_cerrar"  />
                    <rich:componentControl attachTo="esconder-modalimpactosocial" event="onclick" for="modalimpactosocial" operation="hide"/>
                </h:panelGroup>
            </h:form>
        </f:facet>
        <h:panelGrid columns="2">
            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titHabitantesBeneficiados" value="#{var.habitantesbeneficiados}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="verHabitantesBeneficiados" value="#{Supervision$Externa.objetoSeguimiento.inthabbeneficiados}">
                    <f:convertNumber pattern="###,###.##" />
                </h:outputText>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titEmpleosDirectos" value="#{var.empleosdirectos}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="verEmpleosDirectos" value="#{Supervision$Externa.objetoSeguimiento.intempleadosdirectos}">
                    <f:convertNumber pattern="###,###.##" />
                </h:outputText>
            </rich:panel>

            <rich:panel  bodyClass="inpanelBody"  style="background-color:#C1D1E7">
                <h:outputText id="titEmpleosIndirectos" value="#{var.empleosindirectos}:"/>
            </rich:panel>
            <rich:panel  bodyClass="inpanelBody">
                <h:outputText id="verEmpleosIndirectos" value="#{Supervision$Externa.objetoSeguimiento.intempleadosindirectos}">
                    <f:convertNumber pattern="###,###.##" />
                </h:outputText>
            </rich:panel>
        </h:panelGrid>
    </rich:popupPanel>

</html>

